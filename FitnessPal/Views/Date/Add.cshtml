@model FitnessPal.Models.AccountViewModels.AddDateViewModel

<form asp-controller="Date" asp-action="Add" method="post">
    <div class="form-group">
        <label asp-for="Edate"></label>
        <input class="form-control" asp-for="Edate" />
        <span asp-validation-for="Edate"></span>
    </div>
    <div class="form-group">
        <input type="submit" value="Create" class="btn btn-default" />
    </div>
</form>
